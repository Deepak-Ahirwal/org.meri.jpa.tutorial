		<insert tableName="cascadeonetooneinverse">
			<column name="id" valueNumeric="2" />
		</insert>
		<insert tableName="cascadeonetooneowner">
			<column name="id" valueNumeric="2" />
			<column name="inverse_id" valueNumeric="2" />
		</insert>
	</changeSet>

